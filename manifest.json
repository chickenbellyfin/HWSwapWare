{
	"manifest_version": 2,
	"name": "HWSwapWare",
	"version": "1.1",
	"description": "Replaces heatware URLs with heatware feedback stats on /r/hardwareswap.",
	"content_scripts": 
	[
		{
			"matches": ["http://www.reddit.com/r/hardwareswap/*", "https://www.reddit.com/r/hardwareswap/*"],
			"js": ["jquery-2.1.4.min.js", "content_script.js"],
			"run_at": "document_end"
		}
	],
	"permissions":
	[
		"https://www.heatware.com/",
		"https://heatware.com/"
	]
}

