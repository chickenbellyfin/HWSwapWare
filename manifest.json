{
	"manifest_version": 2,
	"name": "HWSwapWare",
	"version": "1.0",
	"description": "Replaces heatware URLs with heatware feedback stats on /r/hardwareswap.",
	"content_scripts": 
	[
		{
			"matches": ["http://www.reddit.com/r/hardwareswap/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"permissions":
	[
		"http://www.heatware.com/",
		"http://heatware.com/"
	]
}

